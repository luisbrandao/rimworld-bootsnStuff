<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef Name="ApparelBootBase" ParentName="ApparelMakeableBase" Abstract="True">
    <generateCommonality>1</generateCommonality>
    <apparel>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Feet</li>
      </bodyPartGroups>
      <layers>
        <li>Middle</li>
      </layers>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>techmago_bootsnStuff</li>
    </thingCategories>
  </ThingDef>
  <ThingDef ParentName="ApparelBootBase"> <!-- Boots -->
    <defName>Apparel_boots</defName>
    <label>Boots</label>
    <description>Hard Boots to protect your foot during work.</description>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/Equipment/Apparel/boots</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>35</costStuffCount>
    <apparel>
      <tags>
        <li>Outlander</li>
      </tags>
    </apparel>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>0.25</Mass>
      <WorkToMake>3000</WorkToMake>
      <StuffEffectMultiplierArmor>0.360</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.050</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.050</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>+0.3</MoveSpeed>
    </equippedStatOffsets>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>RGBA(0.33,0.33,0.33,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>RGBA(0.65,0.65,0.65,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>RGBA(0.4,0.3,0.15,1)</only>
        </li>
        <li>
          <weight>15</weight>
          <min>RGBA(0.3,0.3,0.3,1)</min>
          <max>RGBA(0.5,0.5,0.5,1)</max>
        </li>
        <li>
          <weight>6</weight>
          <min>RGBA(0.5,0.5,0.5,1)</min>
          <max>RGBA(1,1,1,1)</max>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  <ThingDef ParentName="ApparelBootBase"> <!-- Military Boots -->
    <defName>Apparel_military_Boots</defName>
    <label>Military Boots</label>
    <description>High quality boots designed for combatants. Quite expensive and hard to make.</description>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/Equipment/Apparel/bootsMilitary</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>45</costStuffCount>
    <apparel>
      <tags>
        <li>Outlander</li>
      </tags>
    </apparel>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>0.35</Mass>
      <WorkToMake>15000</WorkToMake>
      <StuffEffectMultiplierArmor>0.720</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.080</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.080</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>+0.2</MoveSpeed>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.1</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>RGBA(0.33,0.33,0.33,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>RGBA(0.65,0.65,0.65,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>RGBA(0.4,0.3,0.15,1)</only>
        </li>
        <li>
          <weight>15</weight>
          <min>RGBA(0.3,0.3,0.3,1)</min>
          <max>RGBA(0.5,0.5,0.5,1)</max>
        </li>
        <li>
          <weight>6</weight>
          <min>RGBA(0.5,0.5,0.5,1)</min>
          <max>RGBA(1,1,1,1)</max>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  <ThingDef ParentName="ApparelBootBase"> <!-- Sandals -->
    <defName>Apparel_sandals</defName>
    <label>Sandals</label>
    <description>Simple low tech sandals. Great for warm temperatures.</description>
    <techLevel>Neolithic</techLevel>
    <graphicData>
      <texPath>Things/Item/Equipment/Apparel/sandals</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
    </recipeMaker>
    <costStuffCount>20</costStuffCount>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <tags>
        <li>Tribal</li>
        <li>Outlander</li>
      </tags>
    </apparel>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Mass>0.14</Mass>
      <WorkToMake>1800</WorkToMake>
      <StuffEffectMultiplierArmor>0.0</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.00</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.10</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>+0.4</MoveSpeed>
    </equippedStatOffsets>
  </ThingDef>
  <ThingDef ParentName="ApparelBootBase"> <!-- Shoes -->
    <defName>Apparel_shoes</defName>
    <label>Shoes</label>
    <description>Nice shoes. More elegant, but harder to make.</description>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/Equipment/Apparel/shoes</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>45</costStuffCount>
    <apparel>
      <tags>
        <li>Outlander</li>
        <li>Spacer</li>
      </tags>
    </apparel>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <Mass>0.20</Mass>
      <WorkToMake>15000</WorkToMake>
      <StuffEffectMultiplierArmor>0.18</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.025</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.025</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>+0.3</MoveSpeed>
      <SocialImpact>+0.1</SocialImpact>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.1</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>RGBA(0.33,0.33,0.33,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>RGBA(0.65,0.65,0.65,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>RGBA(0.4,0.3,0.15,1)</only>
        </li>
        <li>
          <weight>15</weight>
          <min>RGBA(0.3,0.3,0.3,1)</min>
          <max>RGBA(0.5,0.5,0.5,1)</max>
        </li>
        <li>
          <weight>6</weight>
          <min>RGBA(0.5,0.5,0.5,1)</min>
          <max>RGBA(1,1,1,1)</max>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  <ThingDef ParentName="ApparelMakeableBase"> <!-- Marine Boots -->
    <defName>Apparel_Marine_Boots</defName>
    <label>Marine Boots</label>
    <description>Marine Boots designed for enhanced performance.</description>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>PoweredArmor</researchPrerequisite>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/Equipment/Apparel/bootsPower</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeTags>
      <li>HiTechArmor</li>
      <li>Armor</li>
    </tradeTags>
    <costList>
      <ComponentSpacer>1</ComponentSpacer>
      <Plasteel>40</Plasteel>
      <Uranium>5</Uranium>
    </costList>
   <apparel>
      <tags>
        <li>Outlander</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
        <li>Spacefarer</li>
      </defaultOutfitTags>
      <useDeflectMetalEffect>true</useDeflectMetalEffect>
      <soundWear>Wear_PowerArmor</soundWear>
      <soundRemove>Remove_PowerArmor</soundRemove>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Feet</li>
      </bodyPartGroups>
      <layers>
        <li>Middle</li>
      </layers>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
    <thingCategories>
      <li>techmago_bootsnStuff</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <Mass>0.5</Mass>
      <WorkToMake>20000</WorkToMake>
      <ArmorRating_Sharp>1.0</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.40</ArmorRating_Blunt>
      <ArmorRating_Heat>0.50</ArmorRating_Heat>
      <Insulation_Cold>60</Insulation_Cold>
      <Insulation_Heat>9</Insulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>+0.2</MoveSpeed>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.1</SlaveSuppressionOffset>
      <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.1</VacuumResistance>
    </equippedStatOffsets>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.9,0.9,0.9)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.65,0.65,0.65)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3)</min>
          <max>(0.5,0.5,0.5)</max>
        </li>
      </options>
    </colorGenerator>
    <comps>
      <li Class="CompProperties_Biocodable"/>
    </comps>
    <thingSetMakerTags>
      <li>RewardStandardHighFreq</li>
    </thingSetMakerTags>
  </ThingDef>
</Defs>

